{
	"name": "react-recipes",
	"version": "1.0.0",
	"description": "A starter for React-Apollo-GraphQL projects",
	"main": "server.js",
	"engines": {
		"node": ">=8.11.1"
	},
	"scripts": {
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "cd client && npm start",
		"dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\"",
		"heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
	},
	"author": "Darko Tasevski <darktasevski@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"apollo-server-express": "^2.0.0",
		"bcrypt": "^3.0.0",
		"body-parser": "^1.18.3",
		"concurrently": "^4.0.1",
		"cors": "^2.8.4",
		"dotenv": "^6.0.0",
		"express": "^4.16.3",
		"graphql": "^14.0.0",
		"graphql-tools": "^3.0.2",
		"jsonwebtoken": "^8.2.1",
		"mongoose": "^5.2.3"
	},
	"devDependencies": {
		"nodemon": "^1.17.5"
	},
	"keywords": []
}
